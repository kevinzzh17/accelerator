  ipgui::add_param $IPINST -name "C_S00_AXIS_TDATA_WIDTH" -parent ${Page_0}
  ipgui::add_param $IPINST -name "PIXEL_WIDTH" -parent ${Page_0}
  ipgui::add_param $IPINST -name "PIX_W" -parent ${Page_0}
  ipgui::add_param $IPINST -name "PIX_H" -parent ${Page_0}


}

